doctype html 
html 
 head 
  title dashboard 
  link(rel='stylesheet' href='/dashbod.css')
 body 
  div 
   header
    h1 Welcome to your dashboard
   nav
    ul
     li
      a(href='/advertise')  Advertise An Apartment
     li 
      a(href='/update') Update An Apartment
     li 
      a(href='/overview') View All Apartment 
     li 
      a(href='/delete') Remove An Apartment
     li 
      a(href='/profile') View Profile 
     li
      a#out(href='')  Click To Logout


 script. 
  document.getElementById('out').addEventListener('click',(e)=>{
       e.preventDefault();
       fetch(`http://localhost:3000/logout`)
       .then(val=>{
        if(val.ok){
            alert(`Logged out successfully`);
            window.setTimeout(()=>location.assign('/'),1000)
        }
       })

    
  })  
